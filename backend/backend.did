type Result = 
 variant {
   err: text;
   ok: nat;
 };
type Comment = 
 record {
   content: text;
   createdAt: int;
   id: nat;
   postId: nat;
 };
type BlogPost = 
 record {
   content: text;
   createdAt: int;
   id: nat;
   title: text;
   updatedAt: opt int;
 };
service : {
  addComment: (nat, text) -> (Result);
  createBlogPost: (text, text) -> (Result);
  getAllBlogPosts: () -> (vec BlogPost) query;
  getBlogPost: (nat) -> (opt BlogPost) query;
  getComments: (nat) -> (vec Comment) query;
}
